{
  "name": "angular2-express-starter",
  "version": "1.0.0",
  "description": "Starter application of Angular2 with Express",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "tsc -p . && npm run _db-copy && concurrently \"tsc -w -p .\" \"nodemon dist/bin/www.js\" ",
    "lint": "tslint './**/*.ts' -c tslint.json --fix",
    "build": "tsc -p . && npm run _db-copy",
    "_db-copy": "cp ./database.json dist/",
    "db-migrate": "db-migrate up --config database.json",
    "db-revert": "db-migrate down --config database.json",
    "db-up": "docker-compose up -d db",
    "postinstall": ""
  },
  "author": "Arturo Volpe",
  "repository": {
    "url": "https://github.com/aVolpe/dev-tools-welcome-page"
  },
  "private": true,
  "dependencies": {
    "@types/compression": "^0.0.33",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.35",
    "@types/jasmine": "2.5.38",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/node": "~6.0.60",
    "@types/uuid": "^2.0.29",
    "bcrypt": "^1.0.2",
    "body-parser": "~1.17.1",
    "class-transformer": "^0.1.6",
    "class-transformer-validator": "^0.2.0",
    "class-validator": "^0.7.0",
    "compression": "^1.6.2",
    "cookie-parser": "~1.3.5",
    "core-js": "^2.4.1",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "global": "^4.3.2",
    "jsonwebtoken": "^7.3.0",
    "path": "^0.12.7",
    "pg-promise": "^5.9.0",
    "ts-helpers": "^1.1.2",
    "ts-node": "~3.0.2",
    "tslint": "^4.5.1",
    "typescript": "^2.2.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "codelyzer": "~2.1.1",
    "concurrently": "^3.1.0",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.0"
  }
}
